#test if statement
if true then {
    print("hi");
}
#bind
IF-STATEMENT:
    COND:
        LITERAL:
            true
    TRUE:
        BLOCK-STATEMENT:
            FN-CALL:
                RECEIVER:
                    SYMBOL:
                        NAME:
                            print
                        SOURCE:
                            GLOBAL:
                                print
                ARGS:
                    LITERAL:
                        "hi"
#result
