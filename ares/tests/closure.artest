#test fibonacci
var fibs = fn() {
  var y = 0;
  var x = 1;
  fn() {
    var aux = y;
    y = x;
    x = x + aux;
    y
  }
};
var next = fibs();
print(next());
print(next());
print(next());
print(next());
print(next());
print(next());
#output
1
1
2
3
5
8

