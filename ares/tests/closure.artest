#test closure counters
var cntr = fn() {
    var i = 0;
    fn() {
        var last = i;
        i = i + 1;
        last
    }
};
var c = cntr();
print(c());
print(c());
print(c());
print(c());
print(c());
print(c());
#output
0
1
2
3
4
5

#test toggler
var toggler = fn(initial) {
    var current = initial;
    fn(new) {
        var prev = current;
        current = new;
        prev
    }
};
var t = toggler(true);
print(t("hello"));
print(t(false));
print(t(1134));
print(t(true));
#output
true
hello
false
1134

